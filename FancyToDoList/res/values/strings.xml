	<string name="WishListDataBase_Create">"
	CREATE TABLE WishItems (_id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT NOT NULL, description TEXT, create_date TEXT, store_id INTEGER, picture BLOB);
	
	CREATE TABLE StoreInfo (store_id INTEGER, store_name TEXT, address TEXT, latitude INTEGER, longitude INTEGER);
	"</string>
	
	<string name="WishListDataBase_Upgrade">"
	DROP TABLE IF EXISTS WishItems
	
	DROP TABLE IF EXISTS StoreInfo
	"</string>
	
